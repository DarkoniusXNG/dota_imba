<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.vcss_c"/>
		<include src="file://{resources}/styles/custom_game/custom_loading_screen.css"/>
	</styles>

	<scripts>
		<include src="file://{resources}/scripts/custom_game/api.js"/>
		<include src="file://{resources}/scripts/custom_game/custom_loading_screen.js"/>
	</scripts>

	<Panel class="addon-root">
		<Panel class="header">
			<Panel class="top-left-container">
				<Panel class="profile-container">

				</Panel>

				<Panel id="vote-container" class="vote-container">
					<Label class="vote-title" text="GAME MODE"/>

					<Label id="VoteGameModeText1" class="vote-label" text="#vote_gamemode_1" />

					<Label id="VoteGameModeText2" class="vote-label" text="#vote_gamemode_2" />

					<Label id="VoteGameModeText3" class="vote-label" text="#vote_gamemode_3" />
<!--
					<Label id="VoteGameModeText4" class="vote-label" text="#vote_gamemode_4" />
-->
					<Label id="VoteGameModeText5" class="vote-label" text="#vote_gamemode_5" />

					<Label id="VoteGameModeCheck" class="vote-label vote-check" text="You have not voted yet." />

					<Button class="vote-change" onactivate="DOTADisplayURL('http://www.dota2imba.fr/home');">
						<Label class="vote-change-label" text="Change Vote" />
					</Button>
				</Panel>
			</Panel>

			<Panel class="description-container">
				<Panel class="description-background">

				</Panel>

				<Panel class="description-text-container">
					<Label id="loading-title-text"/>

					<Label id="loading-subtitle-text" />

					<Label id="loading-description-text" />
				</Panel>
			</Panel>

			<Panel class="battlepass-container">

			</Panel>
		</Panel>

		<Panel class="separator">

		</Panel>

		<Panel class="footer">
			<Panel class="transifex-container">

			</Panel>

			<Panel class="mods-container">

			</Panel>
		</Panel>
	</Panel>
</root>
